{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "tauri": {
    "allowlist": {
      "protocol": {
        "asset": true,
        "assetScope": [
          "$DOCUMENT/**"
        ]
      }
    }
  },
  "security": {
    "csp": null,
    "assetProtocol": {
      "scope": [
        "$TEMP/dimcontrol/**"
      ],
      "enable": true
    }
  },
  "permissions": [
    "core:default",
    "core:app:default",
    "core:event:default",
    "core:path:default",
    "core:image:default",
    "core:image:allow-from-path",
    "core:menu:default",
    "core:window:default",
    "core:tray:default",
    "fs:default"
  ]
}